{
    "name": "Punnycode Detector",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "Punnycode",
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "html/popup.html",
      "default_title": "Protect"
    },
    "background": {
        "scripts": [
            "js/background.js"
        ],
        "persistent": true
    },
    "permissions": ["activeTab"]
  }